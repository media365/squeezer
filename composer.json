{
    "name": "media365/squeezer",
    "description": "url shorten package using Laravel",
    "keywords": [
        "laravel",
        "url",
        "shortening",
        "shortener"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1|^8.0",
        "guzzlehttp/guzzle": "^6.2|^7.0.1",
        "guzzlehttp/psr7": "^1.4|^2.7.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Media365\\Squeezer\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Media365\\Squeezer\\SqueezerServiceProvider"
            ]
        }
    }
}
